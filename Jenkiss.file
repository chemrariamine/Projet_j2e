pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                git 'https://github.com/chemrariamine/Projet_JEE.git'
                sh './mvnw clean compile'
                // bat '.\\mvnw clean compile'
            }
        }
        stage('Test') {
            steps {
                sh './mvnw test'
                // bat '.\\mvnw test'
            }

            post {
                always {
                    junit '**/src/main/webapp/Login.jsp'
                }
            }
        }
    }
}